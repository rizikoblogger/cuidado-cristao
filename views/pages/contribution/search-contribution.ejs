<div id="search-contribution" v-cloak>

  <div class="container">
    <div class="display-3"><%=__('Search Contribution')%></div>
    <hr>

    <table class="table table-striped">
      <caption><%=__('Total')%>: {{page.totalRecords}}</caption>
      <tr>
        <th id="0"></th>
        <th id="1"><%=__('Date')%></th>
        <th id="2"><%=__('Value')%></th>
        <th id="3"><%=__('Propose')%></th>
      </tr>
      <tr v-for="contrib in page.content">
        <td headers="0">
          <a :href="'/contribution/edit-contribution/'+contrib.id" class="btn btn-sm btn-primary"></a>
        </td>
        <td headers="1">{{contrib.dtContribution}}</td>
        <td headers="2">{{contrib.value}}</td>
        <td headers="3">{{contrib.propose}}</td>
      </tr>
    </table>
  </div>
</div>
<%- /* Expose server-rendered data as window.SAILS_LOCALS :: */ exposeLocalsToBrowser() %>
